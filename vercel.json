{
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Content-Type": "text/plain"
      },
      "methods": ["GET"],
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": "Roblox.*"
        }
      ],
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "status": 403,
      "dest": "/403.txt"
    }
  ]
}
